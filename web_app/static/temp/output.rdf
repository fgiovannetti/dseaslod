  <rdf:Description rdf:about="https://example.org/person/Criti">
    <rdf:value rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral">&lt;person sameAs="http://viaf.org/viaf/17263602 http://id.loc.gov/authorities/names/n00087919 http://dbpedia.org/resource/Critias" xml:id="Criti" xmlns="http://www.tei-c.org/ns/1.0"&gt;
            &lt;persName xml:lang="en"&gt;Critias (approximately 460 B.C.-approximately 403 B.C.)&lt;/persName&gt;
            &lt;persName xml:lang="el"&gt;Κριτίας&lt;/persName&gt;
            &lt;event from="-0404-06" to="-0403-01" xml:id="ev02"&gt;
              &lt;desc xml:id="desc02"&gt;Between 404 B.C. and 403 B.C. &lt;persName role="leader"&gt;Critias&lt;/persName&gt; lead the oligarchic government imposed on &lt;placeName ref="#Athens" type="place_of_event"&gt;Athens&lt;/placeName&gt; by &lt;placeName ref="#Sparta"&gt;Sparta&lt;/placeName&gt; after the end of the Peloponnysian war. &lt;bibl xml:id="bibl02"&gt;An accout of the main events concerning this period is given by the &lt;author ref="#Aristot"&gt;aristotelian&lt;/author&gt; &lt;title&gt;Constitution of the Athenians&lt;/title&gt;&lt;/bibl&gt;.&lt;/desc&gt;
            &lt;/event&gt;
          &lt;/person&gt;
          
</rdf:value>
    <owl:sameAs rdf:resource="http://viaf.org/viaf/17263602"/>
    <owl:sameAs rdf:resource="http://id.loc.gov/authorities/names/n00087919"/>
    <rdf:type rdf:resource="https://schema.org/Person"/>
